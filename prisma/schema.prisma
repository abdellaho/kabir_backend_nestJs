  moduleFormat = "cjs"
}

datasource db {
  provider = "mysql"
  //url      = env("DATABASE_URL")
}

model Ville {
  id          					        BigInt         			              @id @default(autoincrement())
  nomVille       				        String      
  etablissements  				      Etablissement[]
  repertoires   				        Repertoire[]
  fournisseurs   				        Fournisseur[]
}

model Fournisseur {
  id              				      BigInt    					              @id @default(autoincrement()) 
  designation		 			          String
  type  				                Int
  tel1             				      String
  tel2             				      String
  ice                           String
  adresse         				      String
  archiver              		    Boolean @default(false)
  supprimer              		    Boolean @default(false)
  ville           				      Ville?     				                @relation(fields: [villeId], references: [id])
  villeId         				      BigInt?
  stocks           			        Stock[]
}

